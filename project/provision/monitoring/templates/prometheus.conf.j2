global:
    scrape_interval: 10s

scrape_configs:
    - job_name: 'prometheus_master'
      scrape_interval: 5s
      static_configs:
        - targets: ['localhost:9090'] 

    - job_name: 'web1'
      scrape_interval: 5s 
      static_configs:
        - targets: ['192.168.255.10:9100']
        - targets: ['192.168.255.11:9100']